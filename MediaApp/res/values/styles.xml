<resources xmlns:android="http://schemas.android.com/apk/res/android" 
    xmlns:rnv="http://schemas.android.com/apk/res-auto">

    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="actionBarStyle">@style/MyTheme.ActionBarStyle</item>
        <item name="actionMenuTextAppearance">@style/MyTheme.ActionBar.MenuTextStyle</item>
        <item name="actionMenuTextColor">@color/app_title_text</item>
       
        
    </style>

    <style name="MyTheme.ActionBarStyle" parent="Widget.AppCompat.Light.ActionBar">
        <item name="titleTextStyle">@style/MyTheme.ActionBar.TitleTextStyle</item>
        <item name="android:background">@null</item>
        
    </style>

    <style name="MyTheme.ActionBar.TitleTextStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/app_title_text</item>
        <item name="android:textSize">20sp</item>
    </style>
   <style name="MyTheme.ActionBar.normalTextStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
    </style>
     <style name="MyTheme.ActionBar.FormSubtitleTextStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
    </style>
    
   
    <style name="MyTheme.ActionBar.MenuTextStyle" parent="TextAppearance.AppCompat.Widget.ActionBar.Menu">
        <item name="android:textColor">@color/app_title_text</item>
        <item name="android:textSize">21sp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textStyle">normal</item>
    </style>

